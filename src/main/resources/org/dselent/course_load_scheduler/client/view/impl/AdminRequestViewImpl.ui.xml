<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HorizontalPanel ui:field="adminRequestPanel">
		<g:VerticalPanel width="359px" height="538px" ui:field="requestListPanel">
			<g:Label text="Request List" ui:field="requestListLabel" horizontalAlignment="ALIGN_CENTER"/>
			<g:ListBox visibleItemCount="5" width="358px" height="201px" ui:field="requestList"/>
		</g:VerticalPanel>
		<g:Cell horizontalAlignment="ALIGN_RIGHT">
			<g:AbsolutePanel width="360px" height="538px" ui:field="replyPanel">
				<g:at top="10" left="10">
					<g:Label text="Request From:" ui:field="fromLabel"/>
				</g:at>
				<g:at top="34" left="10">
					<g:Label text="Type:" ui:field="typeLabel"/>
				</g:at>
				<g:at top="58" left="10">
					<g:Label text="Description:" ui:field="descriptLabel"/>
				</g:at>
				<g:at top="10" left="135">
					<g:Label text="&lt;from&gt;" ui:field="setUserLabel"/>
				</g:at>
				<g:at top="34" left="135">
					<g:Label text="&lt;type&gt;" ui:field="setTypeLabel"/>
				</g:at>
				<g:at top="58" left="135">
					<g:Label text="&lt;description&gt;" ui:field="setUserDescriptLabel"/>
				</g:at>
				<g:at top="195" left="10">
					<g:Label text="Response:" ui:field="responseLabel"/>
				</g:at>
				<g:at top="231" left="10">
					<g:TextArea width="326px" height="121px" ui:field="replyTextArea"/>
				</g:at>
				<g:at top="368" left="10">
					<g:RadioButton name="new name" text="Approved" ui:field="approvedRdo"/>
				</g:at>
				<g:at top="393" left="10">
					<g:RadioButton name="new name" text="Tentative" ui:field="tentativeRdo"/>
				</g:at>
				<g:at top="418" left="10">
					<g:RadioButton name="new name" text="Rejected" ui:field="rejectedRdo"/>
				</g:at>
				<g:at top="454" left="147">
					<g:Button text="Reply" ui:field="replyButton"/>
				</g:at>
			</g:AbsolutePanel>
		</g:Cell>
	</g:HorizontalPanel>
</ui:UiBinder> 