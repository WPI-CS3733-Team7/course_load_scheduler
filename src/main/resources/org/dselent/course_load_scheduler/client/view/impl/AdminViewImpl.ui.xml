<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:VerticalPanel width="100%" height="20%">
		<g:HorizontalPanel width="100%" height="34px">
			<g:SimplePanel width="100%" height="100%"/>
			<g:SimplePanel width="100%" height="100%"/>
			<g:SimplePanel width="100%" height="100%"/>
		</g:HorizontalPanel>
		<g:TabLayoutPanel barHeight="2" barUnit="EM" width="100%" height="221px">
			<g:tab>
			<g:header size='7'><b>Scheduler</b></g:header>
			<g:VerticalPanel>
				<g:HorizontalPanel/>
				<g:HorizontalPanel>
					<g:Button/>
					<g:Button/>
					<g:Button/>
					<g:Button/>
					<g:Button/>
					<g:Button/>
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:tab>
			<g:tab>
				<g:header>Request</g:header>
				<g:AbsolutePanel height="194" width="491">
					<g:at top="35" left="10">
						<g:ListBox visibleItemCount="5" width="207px" height="149px"/>
					</g:at>
					<g:at top="10" left="76">
						<g:Label text="Request List"/>
					</g:at>
					<g:at top="10" left="223">
						<g:AbsolutePanel width="215px" height="74px">
							<g:at top="0" left="10">
								<g:Label text="From:"/>
							</g:at>
							<g:at top="0" left="97">
								<g:Label ui:field="lblRequester" text="&lt;from&gt;"/>
							</g:at>
							<g:at top="24" left="10">
								<g:Label text="Type:"/>
							</g:at>
							<g:at top="24" left="97">
								<g:Label ui:field="lblType" text="&lt;type&gt;"/>
							</g:at>
							<g:at top="48" left="10">
								<g:Label text="Description:"/>
							</g:at>
							<g:at top="48" left="97">
								<g:Label ui:field="lblDescript" text="&lt;descript&gt;"/>
							</g:at>
						</g:AbsolutePanel>
					</g:at>
					<g:at top="88" left="223">
						<g:TextArea width="116px" height="61px"/>
					</g:at>
					<g:at top="90" left="355">
						<g:RadioButton name="new name" text="Approved" ui:field="rdoApproved"/>
					</g:at>
					<g:at top="115" left="355">
						<g:RadioButton name="new name" text="Tentative" ui:field="rdoTentative"/>
					</g:at>
					<g:at top="140" left="355">
						<g:RadioButton name="new name" text="Rejected" ui:field="rdoRejected"/>
					</g:at>
					<g:at top="165" left="308">
						<g:Button text="Reply" ui:field="btnReply"/>
					</g:at>
				</g:AbsolutePanel>
			</g:tab>
			<g:tab>
			<g:header size='7'><b>Account</b></g:header>
			<g:Label>Custom Header</g:Label>
		</g:tab>
		</g:TabLayoutPanel>
		<g:SimplePanel height="100%" width="100%"/>
		
	</g:VerticalPanel>
</ui:UiBinder> 